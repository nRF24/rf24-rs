{
  "name": "@rf24/rf24",
  "version": "0.3.2",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "A npm package binding the nRF24/rf24-rs rust project.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nRF24/rf24-rs.git"
  },
  "homepage": "https://rf24-rs.rtfd.io/en/latest/node-api/",
  "bugs": {
    "url": "https://github.com/nRF24/rf24-rs/issues"
  },
  "funding": [
    "https://github.com/sponsors/2bndy5",
    "https://www.paypal.me/Brendan884"
  ],
  "os": [
    "linux"
  ],
  "napi": {
    "binaryName": "rf24",
    "targets": [
      "x86_64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "i686-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "armv7-unknown-linux-gnueabihf",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-freebsd",
      "universal-apple-darwin"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "3.3.1",
    "@types/node": "24.8.1",
    "prettier": "3.6.2"
  },
  "engines": {
    "node": ">= 10.16.0 < 11 || >= 11.8.0 < 12 || >= 12.0.0"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish --no-gh-release",
    "universal": "napi universalize",
    "version": "napi version"
  },
  "packageManager": "yarn@4.9.2",
  "keywords": [
    "nrf24l01",
    "nRF24L01+",
    "raspberry",
    "pi",
    "linux",
    "driver",
    "radio",
    "transceiver",
    "RF24",
    "BLE"
  ]
}
