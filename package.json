{
    "private": true,
    "workspaces": [
        "bindings/node",
        "docs",
        "examples/node"
    ],
    "scripts": {
        "docs": "cd docs && yarn build",
        "artifacts": "cd bindings/node && yarn artifacts",
        "build": "cd bindings/node && yarn build",
        "build:debug": "cd bindings/node && yarn build:debug",
        "build:examples": "cd examples/node && yarn build",
        "lint": "cd examples/node && yarn lint",
        "universal": "cd bindings/node && yarn universal",
        "format:index": "cd bindings/node && yarn run prettier --write index.*",
        "format:examples": "cd examples/node && yarn run prettier --write */*.{ts,js}"
    },
    "packageManager": "yarn@4.9.2"
}
